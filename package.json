{
    "name": "@seagull/framework",
    "version": "0.1.0",
    "description": "Complete core and utils for developing seagull apps",
    "main": "dist/src/index.js",
    "types": "dist/src/index.d.js",
    "scripts": {
        "build": "rm -rf dist && tsc",
        "test:functional": "NODE_ENV=test mocha --opts test/functional/mocha.opts",
        "test:functional:watch": "npm run test:functional -- --watch",
        "test:unit": "NODE_ENV=test mocha --opts test/unit/mocha.opts",
        "test:unit:watch": "npm run test:unit -- --watch",
        "test": "NODE_ENV=test mocha --opts test/mocha.opts",
        "test:watch": "npm run test -- --watch",
        "docs": "rm -rf docs && typedoc --mode modules --excludeExternals --excludePrivate --excludeNotExported --out docs src"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/seagull-js/framework.git"
    },
    "author": "Maximilian Stroh <hisako1337@gmail.com>",
    "license": "LGPL-3.0",
    "devDependencies": {
        "@types/chai": "^4.1.2",
        "@types/lodash": "^4.14.103",
        "@types/mocha": "^2.2.48",
        "@types/node": "^6.0.100",
        "aws-sdk": "^2.209.0",
        "aws-sdk-mock": "^1.7.0",
        "chai": "^4.1.2",
        "mocha": "^5.0.1",
        "mocha-typescript": "^1.1.12",
        "mock-fs": "^4.4.2",
        "source-map-support": "^0.5.3",
        "ts-node": "^4.1.0",
        "tsconfig-paths": "^3.1.1",
        "tslint": "^5.9.1",
        "tslint-config-prettier": "^1.8.0",
        "typedoc": "^0.11.1",
        "typedoc-plugin-external-module-name": "^1.1.1",
        "typedoc-plugin-single-line-tags": "^1.0.0",
        "typescript": "^2.7.2"
    },
    "dependencies": {
        "lodash": "^4.17.5",
        "typescript-memoize": "^1.0.0-alpha.3",
        "prettier": "^1.10.2",
        "shelljs": "^0.8.1",
        "ts-simple-ast": "^8.1.0"
    },
    "peerDependencies": {
        "aws-sdk": "^2.209.0",
        "typescript": "^2.7.2"
    }
}